# Test Environment Configuration
NODE_ENV=test
PORT=5001

# Test Database (use separate test database)
SUPABASE_URL=your_test_supabase_url
SUPABASE_SERVICE_KEY=your_test_service_key

# JWT Secret (use different secret for tests)
JWT_SECRET=test_jwt_secret_key_for_testing_only

# Disable external services in tests
SENTRY_DSN=
REDIS_URL=
GROQ_API_KEY=

# Email (disabled in tests)
EMAIL_HOST=
EMAIL_PORT=587
EMAIL_USER=
EMAIL_PASS=

# Rate Limiting (more lenient for tests)
BCRYPT_ROUNDS=4
SESSION_TIMEOUT_HOURS=1

# Logging
LOG_LEVEL=error